  componentfrequency = 0.001;
  useExternalSensing = true;
  constantForce = 60.0;
  object1mass = 1.0;
  object1inertia = 0.166667;
}
else{
  componentfrequency = 0.001;
  useExternalSensing = false;
  constantForce = 2.0;
  object1mass = 1.0;
  object1inertia = 0.166667;
}


var ConnPolicy cp;

